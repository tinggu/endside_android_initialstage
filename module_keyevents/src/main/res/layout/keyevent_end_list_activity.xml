<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".activity.KeyEventEndListActivity"
    android:orientation="vertical">

    <include layout="@layout/sub_top_bar"/>

    <fragment
        android:id="@+id/day_calendar_fragment"
        android:name="com.ctfww.commonlib.fragment.DayCalendarFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <fragment
        android:id="@+id/keepevent_list_fragment"
        android:name="com.ctfww.module.keyevents.fragment.KeyEventListFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</LinearLayout>